import React from 'react'
import * as styles from './{{pascalCase name}}.css'

type Props = {
  /** 名前 */
  name: string
  /** state */
  state: string
  /** クリック時の発火関数 */
  handleClick: () => void
}

export const Presenter: React.FC<Props> = ({
  name,
  state,
  handleClick
}) => (
  <div className={ styles.{{camelCase name}} }>
    <p>{name}</p>
    <p>{state}</p>
    <button type='button' onClick={handleClick}>button</button>
  </div>
)
